            <Font name="Papyrus" size="51.0" />
         </font>
      </Text>
      <Button fx:id="StartGameButton" layoutX="480.0" layoutY="640.0" mnemonicParsing="false" onAction="#handleStartGameButton" prefHeight="70.0" prefWidth="180.0" text="Start Game">
         <font>
            <Font name="Papyrus" size="22.0" />
         </font>
      </Button>
      <Button fx:id="ExitGameButton" layoutX="160.0" layoutY="640.0" mnemonicParsing="false" onAction="#handleExitGameButton" prefHeight="70.0" prefWidth="140.0" text=" Exit">
